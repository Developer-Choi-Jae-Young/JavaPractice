package com.kh.practice.list.library.model.vo;

public class Book {
	private String title;
	private String author;
	private String category;
	private int price;
	
	public Book(String title, String author, String category, int price) {
		this.title = title;
		this.author = author;
		this.category = category;
		this.price = price;
	}
	
	public Book() {
	}
	
	
}
